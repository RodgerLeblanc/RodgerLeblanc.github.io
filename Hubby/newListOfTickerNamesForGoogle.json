      "toRemove" : "=X",
      "replaceStar" : "*"
    }
  },
  {
    "[A-Za-z]*=X" : {
      "toRemove" : "=X",
      "replaceStar" : "USD*"
    }
  },
  {
    "\\^DJI" : {
      "toRemove" : "\\^DJI",
      "replaceStar" : ".DJI"
    }
  },
  {
    "\\^IXIC" : {
      "toRemove" : "\\^IXIC",
      "replaceStar" : ".IXIC"
    }
  },
  {
    "\\^GSPC" : {
      "toRemove" : "\\^GSPC",
      "replaceStar" : ".INX"
    }
  },
  {
    "\\^FTSE" : {
      "toRemove" : "\\^FTSE",
      "replaceStar" : ".UKX"
    }
  }
]
